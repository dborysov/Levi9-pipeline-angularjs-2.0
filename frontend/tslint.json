{
    "rules": {
        "align": [
            true,
            "parameters",
            "arguments",
            "statements"
        ],
        "class-name": true,
        "curly": true,
        "eofile": true,
        "indent": "spaces",
        "interface-name": true,
        "member-access": true,
        "member-ordering": [
            true,
            "public-before-private",
            "static-before-instance",
            "variables-before-functions"
        ],
        "no-any": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": true,
        "no-construct": true,
        "no-constructor-vars": true,
        "no-duplicate-key": true,
        "no-duplicate-variable": true,
        "no-inferrable-types": true,
        "no-inferrable-module": true,
        "no-require-imports": true,
        "no-shadowed-variable": true,
        "no-switch-case-fall-through": true,
        "no-trailing-whitespace": true,
        "no-unreachable": true,
        "no-unused-expression": true,
        "no-use-before-declare": true,
        "no-var-keyword": true,
        "no-var-requires": true,
        "object-literal-sort-keys": true,
        "one-line": [
            true,
            "check-open-brace",
            "check-catch",
            "check-else",
            "check-whitespace"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": true,
        "switch-default": true,
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never"
            }
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ]
    }
}